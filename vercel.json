{
  "version": 2,
  "builds": [
    {
      "src": "socket/server/app.js", 
      "use": "@vercel/node"           
    },
    {
      "src": "socket/client/package.json",  
      "use": "@vercel/static-build",      
      "config": {
        "distDir": "socket/client/build"  
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",  
      "dest": "socket/server/app.js"  
    }
  ]
}
